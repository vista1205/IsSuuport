  <DataSets>
    <DataSet Name="Cartridj">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FilterStatus">
            <Value>=Parameters!FilterStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FilterPrint">
            <Value>=Parameters!FilterPrint.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Kartridjs.Title, Kartridjs.NumberOfKartridj, Kartridjs.Image, Kartridjs.StatusID, StatusKartridj.Title AS Expr1, Printers.Title AS Expr2
FROM            Kartridjs INNER JOIN
                         StatusKartridj ON Kartridjs.StatusID = StatusKartridj.ID INNER JOIN
                         Printers ON Kartridjs.PrinterID = Printers.ID
WHERE        (Kartridjs.StatusID = @FilterStatus) AND (Kartridjs.PrinterID = @FilterPrint)</CommandText>
      </Query>
      <Fields>
